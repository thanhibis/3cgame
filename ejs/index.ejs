					<div class="hailoc-content__item on">
						<video autoplay loop muted playsinline>
							<source src="<%= RELATIVE_PATH %>img/thedo.webm" type="video/webm">
						</video>
						<img src="<%= RELATIVE_PATH %>img/1.png" alt="" class="open">
					</div>
					<div class="hailoc-content__item on">
						<video autoplay loop muted playsinline>
							<source src="<%= RELATIVE_PATH %>img/thedo.webm" type="video/webm">
						</video>
						<img src="<%= RELATIVE_PATH %>img/2.png" alt="" class="open">
					</div>
					<div class="hailoc-content__item on">
						<video autoplay loop muted playsinline>
							<source src="<%= RELATIVE_PATH %>img/thedo.webm" type="video/webm">
						</video>
						<img src="<%= RELATIVE_PATH %>img/img-lixi-mo02.png" alt="" class="open">
					</div>
					<div class="hailoc-content__item">
						<video autoplay loop muted playsinline>
							<source src="<%= RELATIVE_PATH %>img/thedo.webm" type="video/webm">
						</video>
						<img src="<%= RELATIVE_PATH %>img/img-lixi-mo02.png" alt="" class="open">
					</div>
					<div class="hailoc-content__item">
						<video autoplay loop muted playsinline>
							<source src="<%= RELATIVE_PATH %>img/thedo.webm" type="video/webm">
						</video>
						<img src="<%= RELATIVE_PATH %>img/img-lixi-mo02.png" alt="" class="open">
